@using PierresBakery.Models;

<h2>@Model["vender"].Name Orders:</h2>

<ol>
  @foreach (Order order in @Model["orders"])
  {
    <li><a href='/vendors/@Model["vender"].Id/orders/@order.Id'>@order.Title</a></li>
  }
</ol>

<p><a href='/venders'>Return to category list</a></p>
<p><a href='/'>Return to Main Page</a></p>